{
  "name": "Sonarr",
  "version": "dev",
  "slug": "hassio_sonarr",
  "description" : "An addon to add sonarr to hassio",
  "url" : "https://github.com/Sabuto/hassio-sonarr",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "before",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:80]",
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": true,
  "host_network": true,
  "host_pid": true,
  "apparmor": false,
  "ingress": true,
  "options": {},
  "schema": {},
  "ports" : {
    "8989/tcp": 8099
  },
  "ports_description" : {
    "80/tcp": "Port for the web interface (not needed with ingress)"
  },
  "image" : "sabuto/{arch}-hassio-sonarr"
}
